<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="654.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button mnemonicParsing="false" onAction="#switchOrderBasket">
      <graphic>
         <ImageView fitHeight="73.0" fitWidth="71.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../Images/icons8-shopping-cart-64.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
    <Button fx:id="donutButton" alignment="CENTER_RIGHT" onAction="#switchDonut" prefHeight="80.0" prefWidth="88.0">
      <graphic>
         <ImageView fitHeight="65.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../Images/icons8-donut-64.png" />
            </image></ImageView>
      </graphic></Button>
   <Button mnemonicParsing="false" onAction="#switchCoffee">
      <graphic>
         <ImageView fitHeight="72.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../Images/icons8-coffee-to-go-64.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button mnemonicParsing="false" onAction="#switchOrderHistory">
      <graphic>
         <ImageView fitHeight="71.0" fitWidth="71.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../Images/icons8-order-history-64.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
</VBox>
